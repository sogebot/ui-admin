{
  "name": "@sogebot/ui-admin",
  "version": "6.1.0",
  "author": "sogehige@gmail.com",
  "engines": {
    "npm": ">7.19.0"
  },
  "description": "Nuxt.js version of ui-admin",
  "scripts": {
    "postinstall": "patch-package || true",
    "release": "standard-version && git push --follow-tags origin main",
    "release:major": "standard-version -r major && git push --follow-tags origin main",
    "prepublishOnly": "npm run generate",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --quiet --ext \".js,.vue\" --fix --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint --quiet --fix",
    "*.{css,vue}": "stylelint"
  },
  "simple-git-hooks": {
    "pre-commit": "npx simple-git-hooks && npx lint-staged",
    "commit-msg": "node ./.bot/tools/pre-commit-message.js $1 $2 $3",
    "preserveUnused": true
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/proxy": "^2.1.0",
    "@skjnldsv/vue-plyr": "^7.0.1-beta.0",
    "@sogebot/ui-helpers": "^2.4.1",
    "animate.css": "^4.1.1",
    "chart.js": "^2.9.4",
    "core-js": "^3.16.1",
    "gsap": "^3.7.1",
    "marked": "^3.0.0",
    "nuxt": "^2.15.8",
    "plyr": "^3.6.8",
    "portal-vue": "^2.1.7",
    "prismjs": "^1.24.1",
    "shortid": "^2.2.16",
    "uuid": "^8.3.2",
    "v-snackbars": "^3.2.4",
    "vue-audio-visual": "^2.3.0",
    "vue-chartkick": "^0.6.1",
    "vue-json-viewer": "^2.2.19",
    "vue-prism-editor": "^1.2.2",
    "vuedraggable": "^2.24.3",
    "xregexp": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sogebot/ui-admin.git"
  },
  "devDependencies": {
    "@mdi/js": "^5.9.55",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/composition-api": "^0.26.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/vuetify": "^1.12.1",
    "@types/chart.js": "^2.9.34",
    "@types/marked": "^2.0.4",
    "@types/prismjs": "^1.16.6",
    "@types/shortid": "^0.0.29",
    "@types/uuid": "^8.3.1",
    "@types/vue-chartkick": "^0.5.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unused-imports": "^1.1.3",
    "eslint-plugin-vue": "^7.16.0",
    "lint-staged": "^11.1.2",
    "obs-websocket-js": "^4.0.3",
    "patch-package": "^6.4.7",
    "simple-git-hooks": "^2.5.1",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "typeorm": "^0.2.37",
    "vuetify": "^2.5.8"
  }
}
